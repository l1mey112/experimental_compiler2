; editing this return value would cause a global effect
; arena_slice :: mut_io [128]const i32

type rmod_t = i32

'arena :: [128]rmod_t
'arena_part :: i32

arena_slice: () = 'arena[0..'arena_part]

is_root :: rmod_t -> bool

is_root: mod = do
    arena_slice
        .for \x -> if x == mod then ret true
    
    ret false